{
    "name": "mgp25/libsignal-php",
    "type": "library",
    "description": "This is a php port of libsignal-java originally written by Moxie Marlinspike",
    "require": {
        "php": ">= 5.6",
        "ext-curve25519": "*",
        "ext-protobuf": "*",
        "ext-crypto": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1"
    },
    "autoload": {
        "psr-4": {
            "Libsignal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Libsignal\\Tests\\": "tests/"
        }
    }
}
